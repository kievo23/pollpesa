<div id="mainnotlogged">
                        <div class="statistics">
                          <div>
                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs" role="tablist">
                              <li role="presentation" class="active"><a href="#results" aria-controls="results" role="tab" data-toggle="tab">Results</a></li>
                              <li role="presentation"><a href="#polls" aria-controls="polls" role="tab" data-toggle="tab">Polls</a></li>
                              <li role="presentation"><a href="#news" aria-controls="news" role="tab" data-toggle="tab">News</a></li>
                            </ul>

                            <!-- Tab panes -->
                            <div class="tab-content">
                              <div role="tabpanel" class="tab-pane active" id="results">


                                  <div class="col-md-2 col-sm-6 col-xs-12 no_padding">
                                      <div class="panel panel-body">
                                          <div class="card-image waves-effect waves-block waves-light">
                                            <i class="fa fa-hand-scissors-o fa-3x" aria-hidden="true"></i>
                                          </div>
                                          <div class="card-reveal">
                                            <span class="card-title grey-text text-darken-4">Last 24 Hours
                                            </span>
                                              {{#if yesterdays}}
                                              <table class="table table-hover table-condensed table-responsive">
                                                <thead>
                                                <tr>
                                                  <th>Votes</th>
                                                  <th>Name</th>
                                                  <th>Odd</th>
                                                  </tr>
                                                </thead>
                                                <tbody>
                                              {{#each yesterdays}}
                                                <tr>
                                                  <td>{{count}}</td>
                                                  <td>{{name}}</td>
                                                  <td>{{odd}}</td>
                                                </tr>
                                              {{/each}}
                                              </table>
                                              {{/if}}
                                          </div>
                                        </div>
                                  </div>
                                  <div class="col-md-2 col-sm-6 col-xs-12 no_padding">
                                     <div class="panel panel-body">
                                        <div class="card-image waves-effect waves-block waves-light">
                                          <i class="fa fa-certificate fa-3x" aria-hidden="true"></i>
                                        </div>
                                        <div class="card-reveal">
                                          <span class="card-title grey-text text-darken-4">Last Seven days
                                          </span>
                                          {{#if lastSevenDays}}
                                            <table class="table table-hover table-condensed table-responsive">
                                              <thead>
                                              <tr>
                                                <th>Votes</th>
                                                <th>Name</th>
                                                <th>Odd</th>
                                                </tr>
                                              </thead>
                                              <tbody>
                                            {{#each lastSevenDays}}
                                              <tr>
                                                <td>{{count}}</td>
                                                <td>{{name}}</td>
                                                <td>{{odd}}</td>
                                              </tr>
                                            {{/each}}
                                            </table>
                                            {{/if}}
                                        </div>
                                      </div>
                                </div>
                                <div class="col-md-2  col-sm-6 col-xs-12 no_padding">
                                    <div class="panel panel-body">
                                        <div class="card-image waves-effect waves-block waves-light">
                                          <i class="fa fa-bar-chart-o fa-3x" aria-hidden="true"></i>
                                        </div>
                                        <div class="card-reveal">
                                          <span class="card-title grey-text text-darken-4">Last Week
                                          </span>
                                            {{#if lastweek}}
                                            <table class="table table-hover table-condensed table-responsive">
                                              <thead>
                                              <tr>
                                                <th>Votes</th>
                                                <th>Name</th>
                                                <th>Odd</th>
                                                </tr>
                                              </thead>
                                              <tbody>
                                            {{#each lastweek}}
                                              <tr>
                                                <td>{{count}}</td>
                                                <td>{{name}}</td>
                                                <td>{{odd}}</td>
                                              </tr>
                                            {{/each}}
                                            </table>
                                            {{/if}}
                                        </div>
                                      </div>
                                </div>
                                <div class="col-md-2 col-sm-6 col-xs-12 no_padding">
                                    <div class="panel panel-body">
                                        <div class="card-image waves-effect waves-block waves-light">
                                          <i class="fa fa-database fa-3x" aria-hidden="true"></i>
                                        </div>
                                        <div class="card-reveal">
                                          <span class="card-title grey-text text-darken-4">Overall
                                          </span>
                                            {{#if overall}}
                                            <table class="table table-hover table-condensed table-responsive">
                                              <thead>
                                              <tr>
                                                <th>Votes</th>
                                                <th>Name</th>
                                                <th>Odd</th>
                                                </tr>
                                              </thead>
                                              <tbody>
                                            {{#each overall}}
                                              <tr>
                                                <td>{{count}}</td>
                                                <td>{{name}}</td>
                                                <td>{{odd}}</td>
                                              </tr>
                                            {{/each}}
                                            </table>
                                            {{/if}}
                                        </div>
                                      </div>
                                </div>


                              </div>
                              <div role="tabpanel" class="tab-pane bright" id="polls" style="padding:20px; ">
                                {{#if polls}}
                                {{#each polls}}
                                    <div class="col-md-3 col-sm-6 col-xs-12 bright">
                                    {{dataValues.name}}
                                    {{dataValues.option}}
                                    </div>
                                {{/each}}
                                {{/if}}
                              </div>
                              <div role="tabpanel" class="tab-pane" id="news" style="padding:20px;">
                                {{#if news}}
                                <div class="panel panel-body">
                                {{#each news}}
                                  <div class="col-md-3 col-sm-6 col-xs-12">
                                    {{dataValues.name}}
                                    {{{dataValues.desc}}}
                                  </div>
                                {{/each}}
                                </div>
                                {{/if}}
                              </div>
                            </div>

                          </div>
                        </div>
            </div>
            <div class="rowd">
  <div class="col-md-10  col-sm-12 col-xs-12 no_margin no_padding">
  <div class="panel">
    {{#if pendingBets}}
      <div class="panel panel-primary">
      <div class="panel panel-body">
      <h3> Pending bets </h3>
        <table class="table table-hover">
          <thead>
            <th>ID</th>
            <th>Amount</th>
            <th>Win Status</th>
            <th>Created At</th>
            <th>Actions</th>
          </thead>
          <tbody>       
            {{#each pendingBets}}
            <tr>
              <td>{{dataValues.id}}</td>
              <td>{{dataValues.amount}}</td>
              <td>{{dataValues.winstatus}}</td>
              <td>{{dataValues.createdAt}}</td>
              <td>              
                <button type="button"class="btn btn-success" value="{{dataValues.id}}" @click="modal({{dataValues.id}})">Details</button>
                <button type="button"class="btn btn-warning" value="{{dataValues.id}}" @click="election({{dataValues.id}})">Run Elections</button>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      </div>
      {{/if}}
    
      <div class="rowd">
        <div class="col-md-12 col-sm-12  col-xs-12 no_padding">

        <div class="panel">
         <p class="text-center">Presidential</p>
        <table class="table table-hover table-condensed table-responsive candidates">
          <thead>
          <tr>
            <th>Name</th>
            <th>Position</th>
             <th>Region</th>
            <th>Actions</th>
            <th>Edit</th>
            </tr>
          </thead>
          <tbody>
          {{#if presidents}}
            {{#each presidents}}
              <tr>
                <td>{{dataValues.name}}</td>
                <td>{{dataValues.position}}</td>                
                    <td>{{dataValues.region.name}}</td>
                <td class="betbutton">
                   <div id="ck-button">
                    <label>
                        <input v-model="cid" type="checkbox" id="cid" name="cid" value="{{dataValues.id}}" @click=candidate><span>{{dataValues.odd}}</span>
                     </label>
                  </div>
                </td>
                <td class="desktop">
                        <a href="candidate/update/{{dataValues.id}}">Edit</a>
                      </td>
              </tr>
            {{/each}}
          {{/if}}
          </tbody>
        </table>
        </div>
      </div>
      </div>
      <div class="">
        <div class="col-md-6 col-sm-12 col-xs-12  no_padding">
          <div class="panel panel-body">
          <p class="text-center">County
            <select name="county" class="form-control">
                <option>--Select County--</option>
                {{#if counties}}
                  {{#each counties}}
                    <option value="{{dataValues.id}}">{{dataValues.name}}</option>
                  {{/each}}
                {{/if}}
            </select>
          </p>
            <table class="table table-hover table-condensed table-responsive candidates">
              <thead>
              <tr>
                <th>Name</th>
                <th>Position</th>
                <th>Region</th>
                <th>Bet</th>
                <th>Edit</th>
                </tr>
              </thead>
              <tbody>
              {{#if candidates}}
                {{#each candidates}}
                  <tr>
                    <td>{{dataValues.name}}</td>
                    <td>{{dataValues.position}}</td>
                    <td>{{dataValues.region.name}}</td>
                    <td class="betbutton">
                       <div id="ck-button">
                        <label>
                            <input v-model="cid" type="checkbox" id="cid" name="cid" value="{{dataValues.id}}" @click=candidate><span>{{dataValues.odd}}</span>
                         </label>
                      </div>
                    </td>
                    <td class="desktop">
                            <a href="candidate/update/{{dataValues.id}}">Edit</a>
                          </td>
                  </tr>
                {{/each}}
              {{/if}}
              </tbody>
            </table>
          </div>  
      </div>
      <div class="col-md-6 col-sm-12 col-xs-12 no_padding">
          <div class="panel panel-body">
          <p class="text-center">Constituency
            <select name="county" class="form-control">
                <option>--Select Constituency--</option>
                {{#if constituencies}}
                  {{#each constituencies}}
                    <option value="{{dataValues.id}}">{{dataValues.name}}</option>
                  {{/each}}
                {{/if}}
            </select>
          </p>
            <table class="table table-hover table-condensed table-responsive candidates">
              <thead>
              <tr>
                <th>Name</th>
                <th>Position</th>
                <th>Region</th>
                <th>Bet</th>
                <th>Edit</th>
                </tr>
              </thead>
              <tbody>
              {{#if mps}}
                {{#each mps}}
                  <tr>
                    <td>{{dataValues.name}}</td>
                    <td>{{dataValues.position}}</td>
                    <td>{{dataValues.region.name}}</td>
                    <td class="betbutton">
                       <div id="ck-button">
                        <label>
                            <input v-model="cid" type="checkbox" id="cid" name="cid" value="{{dataValues.id}}" @click=candidate><span>{{dataValues.odd}}</span>
                         </label>
                      </div>
                    </td>
                    <td class="desktop">
                            <a href="candidate/update/{{dataValues.id}}">Edit</a>
                          </td>
                  </tr>
                {{/each}}
              {{/if}}
              </tbody>
            </table>
          </div>  
      </div>
    </div>
    
  </div>
   </div>
   <div class="col-md-2  col-sm-12 col-xs-12 no_padding no_margin" >
    <div class="panel panel-body  betslip" v-if="candidates.length > 0" id="betslip">
          <hr><h3>Bet Slip</h3> <i class="fa fa-money fa-2x" aria-hidden="true"></i>
          <ul v-if="candidates">
            <table class="table table-hover table-condensed table-responsive">
            <thead>
              <th>Names</th><th>Position</th><th>Odds</th>
            </tbody>
            <tr v-for="candidate in candidates">
            <td>\{{candidate.name}} </td><td>  \{{candidate.position}} </td><td> \{{candidate.odd}}</td>
            </tr>
            </table>
          </ul>
            <div class="panel-footer">
            <form method="post" action="/bet/place" class="form-inline">
              <div class="form-group">
                  Amount to Bet 
                </div>
                <div class="form-group">
                  <input type="text" name="price" id="price" v-model="price" placeholder="Amount to Bet"> 
                  <input type="hidden" name="cids" id="cids" v-model="cid">
                </div>
                <div class="form-group alert alert-success">
                    Anticipated Amount: \{{totalAmount}}
               </div>
                <div class="form-group" v-if="price >= 100">                
                    <button type="submit" class="btn btn-danger">Place Bet</button>
                  </div>
                </form>
               
               </div>
            </div>
            <div class="panel panel-body betslip" v-if="candidates == 0"  id="betslip">
              <hr><h3>Bet Slip</h3> <i class="fa fa-users fa-2x" aria-hidden="true"></i>
              <p class="text-center">You Have Not Selected Any Bets</p>
            </div>
    </div>
    
   </div>
   <modal v-if="showModal" @close="showModal = false">
      
      <h3 slot="header">Bet Details \{{betId}}</h3>
      <table slot="body" class="table table-condensed">
          <thead>
            <th>ID</th>
            <th>odd</th>
            <th>Status</th>
            <th>Name</th>
            <th>Position</th>
            <th>Created At</th>
            <th>Actions</th>
        </thead>
        <tbody>
          <tr v-for="detail in details">
            <td>\{{detail.id}}</td>
            <td>\{{detail.odd}}</td>
            <td>\{{detail.status}}</td>
            <td>\{{detail.name}}</td>
            <td>\{{detail.position}}</td>
            <td>\{{detail.createdAt}}</td>
            <td></td>
          </tr>
        </tbody>
      </table>
    </modal>

  <modal v-if="showProcess" @close="showProcess = false">
      
      <h3 slot="header">Please Wait</h3>
      <div slot="body" ><img src="images/computing.gif" alt="" width="300px"/>
        </div>
        <div slot="footer" ></div>
    </modal>
    <modal v-if="showResult" @close="showResult = false">
      
      <h3 slot="header">MY RESULTS</h3>
      <div slot="body" >
        <img v-bind:src="rstimage" alt="" width="300px"/>
        <br>
        \{{result}}
        </div>
    </modal>
 </div>

